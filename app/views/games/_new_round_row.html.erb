<td> - </td>
<% @team_games.each do |team_game| %>
  <td>
    <h3>
      <span class="badge <%= team_game.total_score == @winning_score ? 'badge-warning' : 'badge-secondary' %>">
        <%= team_game.total_score %>
      </span>
      <br>
      <span class="badge badge-secondary badge-pill" style="background-color: <%= @team_games_with_colors[team_game] %>">
        <%= team_game.team.name %>
      </span>
    </h3>
  </td>
<% end %>
<td>
  <%= form_tag new_round_path, id: 'new-round-form' do %>
    <button title="New round (shift+n)" type="submit" class="btn btn-success"><i class="fa fa-plus"></i></button>
  <% end %>
</td>
<td></td>
